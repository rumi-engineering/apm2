{
  "kind": "vendor.profile",
  "meta": {
    "classification": "INTERNAL",
    "created_at": "2026-01-26T00:00:00Z",
    "dependencies": [],
    "labels": [
      "vendor",
      "export"
    ],
    "provenance": {
      "actor_id": "HOLON-CONTEXT-COMPILER",
      "work_id": "work_example_002"
    },
    "stable_id": "dcp://apm2.local/vendor_profiles/claude_code@v1"
  },
  "payload": {
    "conformance_tests": [
      {
        "params": {
          "path": ".claude/skills"
        },
        "test_id": "T1",
        "test_kind": "path_exists"
      },
      {
        "params": {
          "glob": ".claude/skills/*/SKILL.md",
          "min": 1
        },
        "test_id": "T2",
        "test_kind": "glob_count"
      }
    ],
    "export_contract": {
      "artifacts": [
        {
          "artifact_kind": "skill.spec",
          "mapping": {
            "frontmatter": {
              "generated_by": "apm2 export",
              "source_hash": "{{__source_hash__}}",
              "source_stable_id": "{{meta.stable_id}}"
            },
            "line_endings": "lf",
            "path_template": ".claude/skills/{{payload.skill_name}}/SKILL.md",
            "render_template_id": "tpl_markdown_skill_v1"
          }
        }
      ],
      "root_dir": ".claude/skills"
    },
    "templates": [
      {
        "template_body": "---\nsource_stable_id: {{meta.stable_id}}\nsource_hash: {{__source_hash__}}\nschema: {{schema}}\n---\n\n# {{payload.skill_name}}\n\n## Intent\n{{payload.intent}}\n\n## Instructions\n{% for i in payload.instructions %}- ({{i.id}}) {{i.text}}\n{% endfor %}\n\n## Constraints\n{% for c in payload.constraints %}- {{c}}\n{% endfor %}\n",
        "template_id": "tpl_markdown_skill_v1",
        "template_kind": "markdown"
      }
    ],
    "vendor_id": "claude_code",
    "vendor_profile_version": "v1"
  },
  "schema": "cac.vendor_profile.v1",
  "schema_version": "1.0.0"
}
